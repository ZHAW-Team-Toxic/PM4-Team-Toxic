{
  "format": "RGBA8888",
  "useIndexes": true,
  "filterMin": "Linear",
  "filterMag": "Linear",
  "wrapX": "ClampToEdge",
  "wrapY": "ClampToEdge",
  "edgePadding": true,
  "duplicatePadding": true,
  "rotation": false,
  "stripWhitespaceX": true,
  "stripWhitespaceY": true,
  "alphaThreshold": 0,
  "paddingX": 2,
  "paddingY": 2,
  "maxWidth": 2048,
  "maxHeight": 2048,
  "debug": false,
  "flattenPaths": true,
  "combineSubdirectories": true,
}
